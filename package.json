{
  "name": "ts-template-generator",
  "version": "0.1.0",
  "description": "TypeScript Template Generator",
  "author": "Julio Antúnez Tarín <julio.antunez.tarin@gmail.com>",
  "license": "ISC",
  "main": "src/index.ts",
  "keywords": [
    "javascript",
    "js",
    "node",
    "ts",
    "typescript"
  ],
  "scripts": {
    "***": "<------------------ build -------------------------------------->",
    "commit": "git cz",
    "release": "standard-version --no-verify -t ''"
  },
  "dependencies": {},
  "devDependencies": {
    "commitizen": "^4.2.4",
    "cz-customizable": "^6.3.0",
    "standard-version": "^9.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jatap/ts-template-generator.git"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "cz-customizable.js"
    }
  },
  "engines": {
    "node": "16.x.x",
    "npm": "7.x.x"
  }
}

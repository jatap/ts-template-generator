{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "lib": ["esnext", "es7", "es6", "es2015"],
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": ".compiled/",
    "pretty": true,
    "sourceMap": true,
    "strict": true,
    "target": "es2016",
    "types": ["@types/node"]
  },
  "include": ["generator"],
  "exclude": ["node_modules"]
}